# Backend / database
POSTGRES_PASSWORD=astraforge
POSTGRES_USER=astraforge
POSTGRES_DB=astraforge
POSTGRES_HOST=localhost
POSTGRES_PORT=5433
DATABASE_URL=postgres://astraforge:astraforge@localhost:5433/astraforge
REDIS_URL=redis://localhost:6379/0

# Auth and security
SECRET_KEY=change-me-in-prod
ALLOWED_HOSTS=localhost,127.0.0.1
CSRF_TRUSTED_ORIGINS=http://localhost:8001
UNSAFE_DISABLE_AUTH=1  # local dev only

# LLM / executors
OPENAI_API_KEY=sk-local-placeholder
LLM_MODEL=gpt-4o-mini
EXECUTOR=codex
RUN_LOG_STREAMER=redis
ASTRAFORGE_EXECUTE_COMMANDS=1

# Workspaces / sandbox
CODEX_CLI_SKIP_PULL=1
CODEX_WORKSPACE_IMAGE=ghcr.io/jeremyjouvancedev/astraforge-codex-cli:latest
CODEX_WORKSPACE_PROXY_URL=http://localhost:8080
CODEX_WORKSPACE_NETWORK=astraforge
SANDBOX_IMAGE=ghcr.io/jeremyjouvancedev/astraforge-sandbox:latest
SANDBOX_S3_BUCKET=astraforge-snapshots
SANDBOX_S3_ENDPOINT_URL=http://localhost:9000
SANDBOX_S3_REGION=us-east-1
SANDBOX_S3_USE_SSL=0
AWS_ACCESS_KEY_ID=astraforge
AWS_SECRET_ACCESS_KEY=astraforge123

# Frontend
VITE_API_BASE_URL=http://localhost:8001/api

# Optional helpers
TAVILY_API_KEY=tvly-placeholder

